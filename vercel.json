{
    "framework": "vite",
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "functions": {
        "api/*.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}